@model MyResumeView
 <link rel="stylesheet" type="text/css" href="/assets/plugins/tagcomplete/tagcomplete.css">

<div class="page-content">
        <!-- inner page banner -->
        <div class="overlay-black-dark profile-edit p-t50 p-b20" style="background-image:url(/assets/images/banner/bnr1.jpg);">
            <div class="container">
                <div class="row">
					<div class="col-lg-8 col-md-7 candidate-info">
						<div class="candidate-detail">
							<div class="canditate-des text-center">
								<a href="javascript:void(0);">
									<img alt="" src="/employee/@Model.PhotoUrl">
								</a>
								<div class="upload-link" title="update" data-toggle="tooltip" data-placement="right">
									@* <input type="file" class="update-flie"> *@
									<i class="fa fa-camera"></i>
								</div>
							</div>
							<div class="text-white browse-job text-left">
								<h4 class="m-b0">@Model.Name
									<a class="m-l15 font-16 text-white" data-toggle="modal" data-target="#profilename2" href="#"><i class="fa fa-pencil"></i></a>
								</h4>
								<p class="m-b15">@Model.ProfessionalTitle</p>
								<ul class="clearfix">
									<li><i class="ti-location-pin"></i> @Model.Address, @Model.City</li>
									<li><i class="ti-mobile"></i>@Model.Phone</li>
									<li><i class="ti-briefcase"></i> Fresher</li>
									<li><i class="ti-email"></i> @Model.Email</li>
								</ul>
								<div class="progress-box m-t10">
									<div class="progress-info">Profile Strength (Average)<span>70%</span></div>
									<div class="progress">
										<div class="progress-bar bg-primary" style="width: 80%" role="progressbar"></div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-lg-4 col-md-5">
						<a href="javascript:void(0);">
							<div class="pending-info text-white p-a25">
								<h5>Pending Action</h5>
								<ul class="list-check secondry">
									<li>Verify Mobile Number</li>
									<li>Add Preferred Location</li>
									<li>Add Resume</li>
								</ul>
							</div>
						</a>
					</div>
				</div>
            </div>
			<!-- Modal -->
			<!-- Modal End -->
        </div>
        <!-- inner page banner END -->
		<!-- contact area -->
        <div class="content-block">
			<!-- Browse Jobs -->
			<div class="section-full browse-job content-inner-2">
				<div class="container">
					<div class="row">
						<div class="col-xl-3 col-lg-4 col-md-4 col-sm-12 m-b30">
						
							<div class="sticky-top bg-white">
								<div class="candidate-info onepage">
										
									<ul>
										<li><a class="scroll-bar nav-link" href="#resume_Objective_bx">
											<span>Resume Objective</span></a></li>
										<li><a class="scroll-bar nav-link" href="#key_skills_bx">
											<span>Key Skills</span></a></li>
										<li><a class="scroll-bar nav-link" href="#employment_bx">
											<span>Experience</span></a></li>
										<li><a class="scroll-bar nav-link" href="#education_bx">
											<span>Education</span></a></li>
											<li><a class="scroll-bar nav-link" href="#it_skills_bx">
											<span>Specialization</span></a></li>

										<li><a class="scroll-bar nav-link" href="#projects_bx">
											<span>Other Info</span></a></li>
										<li><a class="scroll-bar nav-link" href="#profile_summary_bx">
											<span>Profile Summary</span></a></li>
										<li><a class="scroll-bar nav-link" href="#accomplishments_bx">
											<span>Online Profile</span></a></li>
										<li><a class="scroll-bar nav-link" href="#desired_career_profile_bx">
											<span>Desired Career Profile</span></a></li>
										<li><a class="scroll-bar nav-link" href="#personal_details_bx">
											<span>Personal Details</span></a></li>
										<li><a class="scroll-bar nav-link" href="#attach_resume_bx">
											<span>Attach Resume</span></a></li>
									</ul>
								</div>
							</div>
						</div>
						<div class="col-xl-9 col-lg-8 col-md-8 col-sm-12">
							
							<div id="resume_Objective_bx" class="job-bx bg-white m-b30">
								<div class="d-flex">
									<h5 class="m-b15">Resume Objective</h5>
									<a href="javascript:void(0);" data-toggle="modal" data-target="#resumeObjective" class="site-button add-btn button-sm"><i class="fa fa-pencil m-r5"></i> Edit</a>
								</div>
								@if(Model.Objective!=null)
								{
								<p class="m-b0">@Model.Objective.Title</p>
								}
								<!-- Modal -->
								<div class="modal fade modal-bx-info editor" id="resumeObjective" tabindex="-1" role="dialog" aria-labelledby="ResumeObjectiveModalLongTitle" aria-hidden="true">
									<div class="modal-dialog" role="document">
										<div class="modal-content">
											<div class="modal-header">
												<h5 class="modal-title" id="ResumeObjectiveModalLongTitle">Resume Objective</h5>
												<button type="button" class="close" data-dismiss="modal" aria-label="Close">
													<span aria-hidden="true">&times;</span>
												</button>
											</div>
												<form asp-action="SetResumeObjective" asp-controller="Employee" method="POST">
											    <div class="modal-body">
												<p>It is the first thing recruiters notice in your profile. Write concisely what makes you unique and right person for the job you are looking for.</p>

													<div class="row">
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<input type="hidden" asp-for="@Model.Objective.Id">
																<input type="hidden" asp-for="@Model.Objective.EmployeeProfileId" value="@Model.Id">
                                                                <textarea class="form-control" asp-for="@Model.Objective.Title" required placeholder="Type Description"></textarea>
																<span asp-validation-for="@Model.Objective.Title" class="text-danger"></span>
															</div>
														</div>
													</div>

											</div>
											<div class="modal-footer">
												<button type="button" class="site-button" data-dismiss="modal">Cancel</button>
												<button type="submit" class="site-button">Save</button>
											</div>
												</form>
										</div>
									</div>
								</div>
								<!-- Modal End -->
							</div>
							<div id="key_skills_bx" class="job-bx bg-white m-b30">
								<div class="d-flex">
									<h5 class="m-b15">Key Skills</h5>
									<a href="javascript:void(0);" data-toggle="modal" data-target="#keyskills" class="site-button add-btn button-sm"><i class="fa fa-pencil m-r5"></i> Edit</a>
								</div>
								<div class="job-time mr-auto">
                                  @if(Model.KeySkill!=null)
								  {
									@foreach(var skill in Model.KeySkill.Skills.Split(','))
									{
									<a href="javascript:void(0);"><span> @skill </span></a>
									}
								  }

								</div>
								<!-- Modal -->
								<div class="modal fade modal-bx-info editor" id="keyskills" tabindex="-1" role="dialog" aria-labelledby="KeyskillsModalLongTitle" aria-hidden="true">
									<div class="modal-dialog" role="document">
										<div class="modal-content">
											<div class="modal-header">
												<h5 class="modal-title" id="KeyskillsModalLongTitle">Key Skills</h5>
												<button type="button" class="close" data-dismiss="modal" aria-label="Close">
													<span aria-hidden="true">&times;</span>
												</button>
											</div>
											<form asp-action="SetKeySkill" asp-controller="Employee" method="POST">
											<div class="modal-body">
												<p>It is the first thing recruiters notice in your profile. Write concisely what makes you unique and right person for the job you are looking for.</p>

													<div class="row">
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<input type="hidden" asp-for="@Model.KeySkill.Id">
																<input type="hidden" asp-for="@Model.KeySkill.EmployeeProfileId" value="@Model.Id">
																 @if(Model.KeySkill!=null)
																 {
																<input type="text" class="form-control tags_input" asp-for="@Model.KeySkill.Skills"/>
																 }
																 else
																 {
															    <input type="text" class="form-control tags_input" asp-for="@Model.KeySkill.Skills" required/>

																 }
															</div>
														</div>
													</div>

											</div>
											<div class="modal-footer">
												<button type="button" class="site-button" data-dismiss="modal">Cancel</button>
												<button type="submit" class="site-button">Save</button>
											</div>
											</form>
										</div>
									</div>
								</div>
								<!-- Modal End -->
							</div>
							<div id="employment_bx" class="job-bx bg-white m-b30 ">
								<div class="d-flex">
									<h5 class="m-b15">Experience</h5>
									<a href="javascript:void(0);" data-toggle="modal" data-target="#employment" class="site-button add-btn button-sm"><i class="fa fa-pencil m-r5"></i> Edit</a>
								</div>
								@if(Model.Experience!=null)
								{
								<h6 class="font-14 m-b0">@Model.Experience.WorkTitle</h6>
								<p class="m-b0">@Model.Experience.CompanyName </p>
								<p class="m-b0">@Model.Experience.FromYear - @Model.Experience.ToYear</p>
								<p class="m-b0">@Model.Experience.Responsibility </p>
								}
								<!-- Modal -->
								<div class="modal fade modal-bx-info editor" id="employment" tabindex="-1" role="dialog" aria-labelledby="EmploymentModalLongTitle" aria-hidden="true">
									<div class="modal-dialog" role="document">
										<div class="modal-content">
											<div class="modal-header">
												<h5 class="modal-title" id="EmploymentModalLongTitle">Add Employment</h5>
												<button type="button" class="close" data-dismiss="modal" aria-label="Close">
													<span aria-hidden="true">&times;</span>
												</button>
											</div>
											<form asp-action="SetExperience" asp-controller="Employee" method="POST">

											<div class="modal-body">


													<input type="hidden" asp-for="@Model.Experience.Id">
													<input type="hidden" asp-for="@Model.Experience.EmployeeProfileId" value="@Model.Id">
													<div class="row">
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Your Designation</label>
																<input type="text"required asp-for="@Model.Experience.WorkTitle" class="form-control" placeholder="Enter Your Designation">
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Your Organization</label>
																<input type="text" required asp-for="@Model.Experience.CompanyName" class="form-control" placeholder="Enter Your Organization">
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Organization Address</label>
																<input type="text" required asp-for="@Model.Experience.CompanyAddress" class="form-control" placeholder="Enter Your Organization">
															</div>
														</div>

														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Started Working From</label>
																<div class="row">
																	<div class="col-lg-6 col-md-6 col-sm-6 col-6">
																		<select required asp-for="@Model.Experience.FromYear">
																			<option>2018</option>
																			<option>2020</option>
																			<option>2016</option>
																			<option>2015</option>
																			<option>2014</option>
																			<option>2013</option>
																			<option>2012</option>
																			<option>2011</option>
																			<option>2010</option>
																			<option>2009</option>
																			<option>2008</option>
																			<option>2007</option>
																			<option>2006</option>
																			<option>2005</option>
																			<option>2004</option>
																			<option>2003</option>
																			<option>2002</option>
																			<option>2001</option>
																		</select>
																	</div>
																	<div class="col-lg-6 col-md-6 col-sm-6 col-6">
																		<select required asp-for="@Model.Experience.FromMonth">
																			<option>January</option>
																			<option>february</option>
																			<option>March</option>
																			<option>April</option>
																			<option>May</option>
																			<option>Jun</option>
																			<option>July</option>
																			<option>August</option>
																			<option>September</option>
																			<option>October</option>
																			<option>November</option>
																			<option>December</option>
																		</select>
																	</div>
																</div>
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Worked Till</label>
																<div class="row">
																	<div class="col-lg-6 col-md-6 col-sm-6 col-6">
																		<select required asp-for="@Model.Experience.ToYear">
																			<option>2018</option>
																			<option>2020</option>
																			<option>2016</option>
																			<option>2015</option>
																			<option>2014</option>
																			<option>2013</option>
																			<option>2012</option>
																			<option>2011</option>
																			<option>2010</option>
																			<option>2009</option>
																			<option>2008</option>
																			<option>2007</option>
																			<option>2006</option>
																			<option>2005</option>
																			<option>2004</option>
																			<option>2003</option>
																			<option>2002</option>
																			<option>2001</option>
																		</select>
																	</div>
																	<div class="col-lg-6 col-md-6 col-sm-6 col-6">
																		<select required asp-for="@Model.Experience.ToMonth">
																			<option>January</option>
																			<option>february</option>
																			<option>March</option>
																			<option>April</option>
																			<option>May</option>
																			<option>Jun</option>
																			<option>July</option>
																			<option>August</option>
																			<option>September</option>
																			<option>October</option>
																			<option>November</option>
																			<option>December</option>
																		</select>
																	</div>
																</div>
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group" >
																<label>Describe your Responsibility</label>
																<textarea required class="form-control" asp-for="@Model.Experience.Responsibility" placeholder="Type Description"></textarea>
															</div>
														</div>
													</div>

											</div>
											<div class="modal-footer">
												<button type="button" class="site-button" data-dismiss="modal">Cancel</button>
												<button type="submit" class="site-button">Save</button>
											</div>
											</form>


										</div>



									</div>
								</div>
								<!-- Modal End -->
							</div>
							<div id="education_bx" class="job-bx bg-white m-b30">
								<div class="d-flex">
									<h5 class="m-b15">Education</h5>
									<a href="javascript:void(0);" data-toggle="modal" data-target="#education" class="site-button add-btn button-sm"><i class="fa fa-pencil m-r5"></i> Edit</a>
								</div>
								<p>Mention your Education details including your College, University and CGPA</p>
								<!-- Modal -->
								<div class="modal fade modal-bx-info editor" id="education" tabindex="-1" role="dialog" aria-labelledby="EducationModalLongTitle" aria-hidden="true">
									<div class="modal-dialog" role="document">
										<div class="modal-content">
											<div class="modal-header">
												<h5 class="modal-title" id="EducationModalLongTitle">Education</h5>
												<button type="button" class="close" data-dismiss="modal" aria-label="Close">
													<span aria-hidden="true">&times;</span>
												</button>
											</div>
										<form asp-action="SetEducation" asp-controller="Employee" method="POST">
											<div class="modal-body">
                                                    <input type="hidden" asp-for="@Model.Education.Id">
													<input type="hidden" asp-for="@Model.Education.EmployeeProfileId" value="@Model.Id">
													<div class="row">
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Education</label>
																<select asp-for="@Model.Education.CourseTitle">
																	<option>Doctorate/PhD</option>
																	<option>Masters/Post-Graduation</option>
																	<option>Graduation/Diploma</option>
																</select>
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>College</label>
																<input type="text" class="form-control" asp-for="@Model.Education.College" placeholder="Select College">
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>University/Institute</label>
																<input asp-for="@Model.Education.University" class="form-control" placeholder="Select University/Institute">
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Graduation Year</label>
																	<select asp-for="@Model.Education.GraduatedDate">
																			<option>2018</option>
																			<option>2020</option>
																			<option>2016</option>
																			<option>2015</option>
																			<option>2014</option>
																			<option>2013</option>
																			<option>2012</option>
																			<option>2011</option>
																			<option>2010</option>
																			<option>2009</option>
																			<option>2008</option>
																			<option>2007</option>
																			<option>2006</option>
																			<option>2005</option>
																			<option>2004</option>
																			<option>2003</option>
																			<option>2002</option>
																			<option>2001</option>
																		</select>
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>CGPA</label>
																<input asp-for="@Model.Education.CGPA" class="form-control" placeholder="CGPA">
															</div>
														</div>

													</div>

											</div>
											<div class="modal-footer">
												<button type="button" class="site-button" data-dismiss="modal">Cancel</button>
												<button type="submit" class="site-button">Save</button>
											</div>
										</form>
										</div>
									</div>
								</div>
								<!-- Modal End -->
								@if(Model.Education!=null)
								{
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12">
										<div class="clearfix m-b20">
											<label class="m-b0">@Model.Education.College</label>
											<span class="clearfix font-13">Student of <strong>@Model.Education.GraduatedDate</strong></span>
										</div>

									</div>
								</div>
								<a class="clearfix">@Model.Education.CourseTitle</a>
								<a class="clearfix">@Model.Education.University</a>
								<a class="clearfix">@Model.Education.CGPA</a>
								}
							</div>
							<div id="it_skills_bx" class="job-bx table-job-bx bg-white m-b30">
								<div class="d-flex">
									<h5 class="m-b15">Specialization</h5>
									<a href="javascript:void(0);" data-toggle="modal" data-target="#itskills" class="site-button add-btn button-sm"><i class="fa fa-pencil m-r5"></i> Edit</a>
								</div>
								<p>Mention your Specialization details including your current and previous company work experience</p>
								@if(Model.Specialization!=null)
								{
								<table>
									<thead>
										<tr>
											<th>SN</th>
											<th>Specialization</th>
											<th>Experience</th>
											<th></th>
										</tr>
									</thead>
									<tbody>
										<tr>
											<td>1</td>
											<td>@Model.Specialization.Specialization1</td>
											<td>2018</td>

											<td><a class="m-l15 font-14" data-toggle="modal" data-target="#itskills" href="#"><i class="fa fa-pencil"></i></a></td>
										</tr>
										<tr>
											<td>2</td>
											<td>@Model.Specialization.Specialization2</td>
											<td>2018</td>

											<td><a class="m-l15 font-14" data-toggle="modal" data-target="#itskills" href="#"><i class="fa fa-pencil"></i></a></td>
										</tr>
										<tr>
											<td>3</td>
											<td>@Model.Specialization.Specialization3</td>
											<td>2018</td>

											<td><a class="m-l15 font-14" data-toggle="modal" data-target="#itskills" href="#"><i class="fa fa-pencil"></i></a></td>
										</tr>




									</tbody>
								</table>
								}
								<!-- Modal -->
								<div class="modal fade modal-bx-info editor" id="itskills" tabindex="-1" role="dialog" aria-labelledby="ItskillsModalLongTitle" aria-hidden="true">
									<div class="modal-dialog" role="document">
										<div class="modal-content">
											<div class="modal-header">
												<h5 class="modal-title" id="ItskillsModalLongTitle">Specialization</h5>
												<button type="button" class="close" data-dismiss="modal" aria-label="Close">
													<span aria-hidden="true">&times;</span>
												</button>
											</div>
											<form asp-action="SetSpecialization" asp-controller="Employee" method="POST">
											<div class="modal-body">
													<input type="hidden" asp-for="@Model.Specialization.Id">
													<input type="hidden" asp-for="@Model.Specialization.EmployeeProfileId" value="@Model.Id">

													<div class="row">

														<div class="col-lg-12 col-md-12">
															<p>Any 3 Specialization</p>
															<div class="form-group">
																<label>Specialization</label>
																<input type="text" required asp-for="@Model.Specialization.Specialization1" class="form-control" placeholder="Enter Specialization">
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
															<div class="form-group">
																<label>Specialization</label>
																<input type="text" required asp-for="@Model.Specialization.Specialization2" class="form-control" placeholder="Enter Specialization">
															</div>
														</div>
														<div class="col-lg-12 col-md-12">
